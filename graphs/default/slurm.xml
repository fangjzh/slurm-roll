	<!-- add edges here -->

	<edge from="base">
	  <to>slurm-base</to>
	</edge>

	<edge from="server">
	  <to>slurm-server</to>
	</edge>

	<edge from-"client">
	  <to>slurm-client</to>
	</edge>

	<!-- enforce post section ordering -->

	<!--
	  slurm-server post sections execute before slurm-base
	  post sections
	-->	
	<order head="slurm-server">
	  <tail>slurm-base</tail>
	</order>

	<!--
	  slurm-client post sections execute after slurm-base
	  post sections
	-->
	<order head="slurm-base">
	  <tail>slurm-client</tail>
	</order>

</graph>
